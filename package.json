{
  "version": "0.1.0",
  "name": "bk-contentful-demo",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "analyze": "cross-env ANALYZE=true NODE_ENV=production next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:size": "cross-env ANALYZE=true NODE_ENV=production next build && node scripts/analyze-bundles.js",
    "build:prepare": "cp ./tsconfig.default.json ./tsconfig.json",
    "build:sourcemap": "cross-env GENERATE_SOURCEMAP=true next build",
    "build:analyze": "yarn build:sourcemap && yarn analyze",
    "deploy:site": "yarn build:prepare && vercel --prod",
    "deploy:storybook": "yarn storybook:prepare && yarn chromatic --auto-accept-changes",
    "build": "yarn build:prepare && next build",
    "deploy": "yarn deploy:site && yarn deploy:storybook",
    "start": "next start",
    "lint": "echo \"Linting... \" && next lint && yarn lint:css",
    "lint:css": "stylelint **/*.{scss,css}",
    "lint:css:fix": "stylelint --fix **/*.{scss,css}",
    "storybook": "yarn storybook:prepare && start-storybook -p 6006 -s ./public",
    "storybook:prepare": "cp ./tsconfig.storybook.json ./tsconfig.json",
    "build-storybook": "build-storybook -s public",
    "chromatic": "dotenv -c npx chromatic",
    "check-circular-deps": "echo \"Checking for Circular dependencies... \" && madge --circular --extensions ts ./",
    "commit": "cz",
    "codegen": "graphql-codegen --config codegen.yml",
    "contentful:export": "shx rm -f contentful/* && contentful space export --export-dir contentful",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "webpack": "^5"
  },
